pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               sh 'cd coit-frontend && docker build -t mkothapally/coit-frontend:v1.4 -f Dockerfile-multistage .'
            }
        }
        stage('docker-push') {
            steps {
                sh 'docker push mkothapally/coit-frontend:v1.4'
            }
    }
}
}
